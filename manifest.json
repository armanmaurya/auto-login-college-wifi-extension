{
  "manifest_version": 3,
  "name": "Auto Wi-Fi Login",
  "version": "1.0.0",
  "description": "Automatically log into 192.168.1.254 captive portal.",
  "permissions": ["storage", "scripting", "tabs", "webNavigation", "alarms"],
  "host_permissions": ["https://192.168.1.254/*"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Auto Wi-Fi Login"
  }
}